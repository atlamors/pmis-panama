    "clean": "rimraf .next out node_modules"
  },
  "dependencies": {
    "next": "^15.1.0",
    "nextra": "^4.4.0",
    "nextra-theme-docs": "^4.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "pagefind": "^1.4.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.2"
  }
}
